{
    "version": "2.0",
    "logging":
    {
        "applicationInsights":
        {
            "samplingSettings":
            {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },

    "retry":
    {
        "strategy": "exponentialBackoff",
        "maxRetryCount": 5,
        "minimumInterval": "00:00:05",
        "maximumInterval": "00:05:00"
    }
}